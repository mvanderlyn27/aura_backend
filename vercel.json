{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist",
        "includeFiles": [
          "dist/**",
          "vercel-start.js",
          "tsconfig.json",
          "package.json",
          "node_modules/**"
         ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel-start.js"
    }
  ]
}
