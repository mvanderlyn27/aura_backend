{
  "name": "aura_backend",
  "version": "1.0.0",
  "description": "Fastify backend for Aura deployed on Google Cloud Run",
  "main": "build/index.js",
  "scripts": {
    "test": "tap test/**/*.test.ts --allow-incomplete-coverage --allow-empty-coverage",
    "build": "rm -rf build/; npx tsc -p tsconfig.json",
    "start": "node build/index.js",
    "develop": "ts-node-dev --respawn --transpile-only --exit-child index.ts",
    "loadtest": "autocannon http://localhost:3000/api/status/ping",
    "clinic:doctor": "clinic doctor --on-port 'autocannon localhost:3000/api/status/ping' -- node build/index.js",
    "clinic:bubbleprof": "clinic bubbleprof --on-port 'autocannon localhost:3000/api/status/ping' -- node build/index.js",
    "clinic:flame": "clinic flame --on-port 'autocannon localhost:3000/api/status/ping' -- node build/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/caching": "^9.0.3",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/swagger": "latest",
    "@fastify/swagger-ui": "latest",
    "@fastify/websocket": "^11.0.2",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google/generative-ai": "^0.24.0",
    "@mgcrea/fastify-request-logger": "^1.4.0",
    "@mgcrea/pino-pretty-compact": "^1.3.0",
    "@supabase/supabase-js": "^2.49.4",
    "dotenv": "^16.5.0",
    "fastify": "latest",
    "fastify-plugin": "^5.0.1",
    "json-schema-to-ts": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/uuid": "^10.0.0",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "concurrently": "^8.2.0",
    "fastify-cli": "^7.4.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^13.0.0",
    "tap": "^21.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
